fun main() {
    //canciones de nene malo
    var nombre = arrayOf("nena","es un secreto","el garrote", "contigo amanecer", "danza dembow", "atrevida")
    var count = 0
	var letra = "e"
    
	for(i in nombre.indices){
        for(k in nombre[i]){
            if (k.toString() == letra){
                println(nombre[i]) 
                count++
                break
            }
        }
    }	
   println(count)
}
